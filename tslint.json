{
	"extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
	"rules": {
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"no-debugger": false,
		"no-console": false,
		"member-access": false,
		"jsx-no-lambda": false
	},
	"linterOptions": {
		"exclude": ["config/**/*.js", "node_modules", "coverage/lcov-report/*.js"]
	}
}
